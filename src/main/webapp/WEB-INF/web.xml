<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
        <servlet-name>SaveFeedback</servlet-name>
        <servlet-class>business_logic.SaveFeedback</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SaveTestimonials</servlet-name>
        <servlet-class>business_logic.SaveTestimonials</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>SaveFeedback</servlet-name>
        <url-pattern>/SaveFeedback</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SaveTestimonials</servlet-name>
        <url-pattern>/SaveTestimonials</url-pattern>
    </servlet-mapping>
    
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
  
  <welcome-file-list>
  <welcome-file>Home.jsp</welcome-file>
  </welcome-file-list>
</web-app>
